{
    "expo": {
        "name": "YourAppName",
        "slug": "your-app-slug",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "newArchEnabled": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            },
            "newArchEnabled": true
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "developmentClient": true
    }
}
